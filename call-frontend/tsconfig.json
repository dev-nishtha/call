{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
      "jsx": "react-native",
      "lib": ["dom", "esnext"],
      "moduleResolution": "node",
      "skipLibCheck": true,
      "resolveJsonModule": true
      
  
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
